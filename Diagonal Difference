#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main(){ 

    int n,i,j,sum1=0,sum2=0,s;
    unsigned int a[100][100];
    scanf("%d",&n);
    for(i=0;i<n;i++)
        {
        for(j=0;j<n;j++)
            {
            scanf("%u",&a[i][j]);
            if(i==j)
                {
                sum1=sum1+a[i][j];
            }
            
            
            }
    }
        for(j=0;j<n;j++)
            {
            sum2=sum2+a[j][n-j-1];
        }
        
        
  
        s=sum1-sum2;
    if(s<0)
    printf("%d",-1*s);
    else
        printf("%d",s);
    
    
    return 0;
}
