#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    int n,i,a[100];
    float p,q,r,c1=0.0,c2=0.0,c3=0.0;
    scanf("%d",&n);
    for(i=0;i<n;i++)
        { 
       scanf("%d",&a[i]);
    if(a[i]>0)
        c1++;
    else if(a[i]<0)
        c2++;
        else if(a[i]==0)
        c3++;
        }
       
p=c1/n;
q=c2/n;
r=c3/n;
printf("%0.3f",p);
printf("\n%0.3f",q);
printf("\n%0.3f",r);
    return 0;
}
